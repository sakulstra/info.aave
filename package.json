{
  "name": "aave-blitz",
  "version": "1.0.0",
  "scripts": {
    "dev": "blitz dev",
    "deploy": "NODE_ENV=production blitz prisma migrate deploy",
    "build": "npm run prisma:generate && blitz build",
    "disabled-post-build": "DATABASE_URL=${RAW_DATABASE_URL} NODE_ENV=production blitz prisma migrate deploy",
    "start": "blitz start -p $PORT",
    "studio": "blitz prisma studio",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "prisma:migrate": "blitz prisma migrate dev",
    "prisma:migrate:create": "blitz prisma migrate dev --create-only",
    "prisma:generate": "blitz prisma generate",
    "prisma:version": "prisma -v",
    "prepare": "husky install",
    "mongo": "mongod --quiet --replSet rs0 --dbpath ../database"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged",
      "pre-push": "tsc && npm run lint && npm run test"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@aave/contract-helpers": "0.15.1",
    "@aave/math-utils": "1.7.0",
    "@aave/protocol-js": "2.7.3-6f5d7ec539a34b2f5c8943145151b1d0646a45fe.0",
    "@emotion/cache": "11.6.0",
    "@emotion/react": "11.7.0",
    "@emotion/server": "11.4.0",
    "@emotion/styled": "11.6.0",
    "@genql/runtime": "2.8.0",
    "@mui/icons-material": "5.2.1",
    "@mui/lab": "5.0.0-alpha.59",
    "@mui/material": "5.2.3",
    "@mui/styles": "5.2.3",
    "@prisma/client": "3.6.0",
    "blitz": "0.44.3",
    "clsx": "1.1.1",
    "dayjs": "1.10.7",
    "ethers": "5.5.2",
    "graphql": "16.0.1",
    "lodash": "4.17.21",
    "mongodb": "4.2.1",
    "node-cron": "3.0.0",
    "prisma": "3.6.0",
    "react": "0.0.0-experimental-3310209d0",
    "react-dom": "0.0.0-experimental-3310209d0",
    "tslib": "2.3.1",
    "zod": "3.11.6"
  },
  "devDependencies": {
    "@genql/cli": "2.8.0",
    "@types/mongodb": "4.0.7",
    "@types/node-cron": "3.0.0",
    "@types/react": "17.0.27",
    "eslint": "8.4.1",
    "husky": "^7.0.4",
    "lint-staged": "12.1.2",
    "prettier": "2.5.1",
    "prettier-plugin-prisma": "3.6.0",
    "pretty-quick": "3.1.2",
    "ts-node": "10.4.0",
    "typescript": "4.5.2"
  },
  "private": true
}
